@model UserLogin

<h1>Login</h1>

<form method="post" asp-action="Login">
    <div>
        <label>Username:</label>
        <input type="text" asp-for="Username" />
    </div>

    <div>
        <label>Password:</label>
        <input type="password" asp-for="Password" />
    </div>

    <input type="submit" value="submit"/>
</form>

